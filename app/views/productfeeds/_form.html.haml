= form_for @productfeed do |f|
  -if @productfeed.errors.any?
    #error_explanation
      %h2= "#{pluralize(@productfeed.errors.count, "error")} prohibited this productfeed from being saved:"
      %ul
        - @productfeed.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :id
    = f.number_field :id
  .field
    = f.label :link
    = f.text_field :link
  .field
    = f.label :summary
    = f.text_area :summary
  .field
    = f.label :title
    = f.text_field :title
  .field
    = f.label :updated
    = f.datetime_select :updated
  .actions
    = f.submit 'Save'
